{
  "name": "react-app-electron-template",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^5.6.3",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.3.0"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "start": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000/ && cross-env NODE_ENV=development yarn electron-start\"",
    "build": "yarn react-build && yarn build-main && copyfiles package.json build/ && yarn electron-build",
    "electron-start": "electron .",
    "electron-build": "electron-packager ./build --out=dist",
    "react-start": "craco start",
    "react-build": "craco build",
    "build-main": "webpack --config electron/webpack.config.js",
    "test": "craco test"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@storybook/react": "^5.3.9",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.0",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.2",
    "copyfiles": "^2.2.0",
    "cross-env": "^7.0.0",
    "electron": "^9.4.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-packager": "^14.2.0",
    "source-map-explorer": "^2.2.2",
    "typescript": "^3.7.5",
    "wait-on": "^4.0.0",
    "webpack-cli": "^3.3.11"
  }
}
