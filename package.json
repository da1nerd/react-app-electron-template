{
  "name": "react-app-electron-template",
  "version": "0.1.0",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "dependencies": {
    "@craco/craco": "^6.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "storybook": "start-storybook -p 6006 -s public",
    "start": "concurrently \"yarn react-start\" \"wait-on http://localhost:3000/ && cross-env NODE_ENV=development yarn electron-start\"",
    "build": "yarn react-build && yarn build-main && copyfiles package.json build/ && yarn electron-build",
    "electron-start": "electron .",
    "electron-build": "electron-packager ./build --out=dist",
    "react-start": "craco start",
    "react-build": "craco build",
    "build-main": "webpack --config electron/webpack.config.js",
    "test": "craco test",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "engines": {
    "node": ">= 10.16"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/node-logger": "^6.2.9",
    "@storybook/preset-create-react-app": "^3.1.7",
    "@storybook/react": "^6.2.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.0.1",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "concurrently": "^6.0.2",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "electron": "^15.5.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^15.2.0",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.2.4",
    "wait-on": "^5.3.0",
    "webpack-cli": "^4.6.0"
  }
}
